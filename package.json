{
  "name": "ts-batch-processor",
  "version": "1.2.3",
  "description": "A batch processor with event emitter for browser and node.js",
  "keywords": [
    "batch",
    "batch-js",
    "batch-ts",
    "batch-browser",
    "batch-node",
    "batch-processor",
    "js-batch",
    "js-batch-processor",
    "ts-batch",
    "ts-batch-processor",
    "batch-processor-js",
    "batch-processor-ts",
    "batch-processor-browser",
    "batch-processor-node",
    "batch-processor-browser-ts",
    "batch-processor-node-ts",
    "batch-processor-browser-js",
    "batch-processor-node-js",
    "concurrency",
    "concurrency-batch-processor",
    "concurrency-batch-processor-js",
    "concurrency-batch-processor-ts",
    "concurrency-batch-processor-browser",
    "concurrency-batch-processor-node",
    "concurrency-batch-processor-browser-ts"
  ],
  "license": "MIT",
  "author": "Jaswanth Sai Sattenapalli <jaswanthsaisattenapalli@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/constants/errors.js",
      "require": "./dist/constants/errors.js",
      "types": "./dist/constants/errors.d.ts"
    },
    "./events": {
      "import": "./dist/events.js",
      "require": "./dist/events.js",
      "types": "./dist/events.d.ts"
    },
    "./task": {
      "import": "./dist/types/task.js",
      "require": "./dist/types/task.js",
      "types": "./dist/types/task.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "build:test": "npm run build && npm test"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jassu225/ts-batch"
  },
  "homepage": "https://github.com/Jassu225/ts-batch/blob/main/README.md",
  "publishConfig": {
    "access": "public"
  }
}
